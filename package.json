    "schema.json"
  ],
  "homepage": "https://github.com/namics/nitro-frontify-deployer#readme",
  "dependencies": {
    "@frontify/frontify-api": "^0.2.0",
    "@namics/nitro-component-resolver": "^0.0.3",
    "@namics/nitro-component-validator": "^0.0.2",
    "denodeify": "^1.2.1",
    "html": "0.0.10",
    "lodash": "^4.13.1",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.5.2"
  },
  "devDependencies": {
    "@namics/eslint-config": "^0.1.0",
    "ava": "^0.15.2",
    "babel-eslint": "^6.1.0",
    "coveralls": "^2.11.9",
    "eslint": "^2.13.1",
    "eslint-plugin-import": "^1.10.0",
    "nyc": "^6.6.1",
    "ncp": "^2.0.0",
    "npm-run-all": "^2.3.0"
